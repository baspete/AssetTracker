{
  "name": "flighttracker",
  "description": "A simple tracking website for PiAware",
  "version": "1.0.0",
  "author": "Pete Butler <pete.butler@ge.com>",
  "license": "MIT",
  "private": true,
  "main": "server.js",
  "scripts": {
    "dev": "npm run server | npm run vue",
    "server": "pm2 start server.js --watch --name server",
    "vue": "cross-env NODE_ENV=development pm2 start webpack-dev-server --name vue -- --open --hot --port=9001",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
